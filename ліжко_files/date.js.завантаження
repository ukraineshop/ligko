function dateToYMD(e){let t=e.getDate(),a=e.getMonth()+1;return(t<=9?"0"+t:t)+"."+(a<=9?"0"+a:a)}function setDateInClass(e,t){let a=document.getElementsByClassName(e);t=dateToYMD(t);for(let e=0;e<a.length;e++)a[e].innerText=t}let dateFrom=new Date,dateTo=new Date;dateFrom.setDate((new Date).getDate()-7),dateTo.setDate((new Date).getDate()+7),setDateInClass("date-from",dateFrom),setDateInClass("date-to",dateTo);